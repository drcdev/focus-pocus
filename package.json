    "omnifocus",
    "mcp",
    "productivity",
    "task-management"
  ],
  "author": "",
  "license": "ISC",
  "description": "OmniFocus MCP server for AI assistant integration",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "jest": "^30.0.5",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "ts-jest": "^29.4.1"
  }
}
